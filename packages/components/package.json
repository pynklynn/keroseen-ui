{
  "name": "keroseen-ui-dev",
  "version": "1.0.0-alpha.0",
  "description": "A minimal design system to develop a front-end library",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass:old": "sass --style compressed demo/sass/main.scss docs/demo.css",
    "sass:dev:old": "sass --watch demo/sass/main.scss docs/demo.css",
    "build:scss": "sass --style compressed tests/scss/index.scss tests/dist/index.css",
    "build:scss:dev": "sass tests/scss/index.scss tests/dist/index.css",
    "watch:scss": "sass --watch tests/scss/index.scss tests/dist/index.css",
    "build": "concurrently \"npm:sass\" \"npm:pug\" \"npm:imagemin\"",
    "build:dev": "concurrently \"npm:sass\" \"npm:sass:dev\" \"npm:pug:dev\" \"npm:imagemin\"",
    "pug": "pug -o docs/ demo/pug",
    "pug:dev": "pug -o docs/ -w -P demo/pug",
    "imagemin": "imagemin demo/images --out-dir docs/images",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pynklynn/keroseen-ui.git"
  },
  "keywords": [
    "scss",
    "sass",
    "ui",
    "css"
  ],
  "author": "Will Steinmetz <willsteinmetz@users.noreply.github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pynklynn/keroseen-ui/issues"
  },
  "homepage": "https://github.com/pynklynn/keroseen-ui#readme",
  "devDependencies": {
    "sass": "^1.63.4",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@pynklynn/backdraft": "^0.1.0",
    "lit": "^2.7.5"
  },
  "directories": {
    "test": "tests"
  }
}
