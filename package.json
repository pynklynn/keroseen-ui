{
  "name": "keroseen-ui",
  "version": "0.2.2",
  "description": "Sass framework of minimal includes to use as a blueprint to create a custom UI without the bloat",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass": "node-sass --output-style compact --source-map true src/demo/sass/main.scss demo/demo.css",
    "sass:dev": "node-sass --output-style nested --source-map true --watch src/demo/sass/main.scss demo/demo.css",
    "build": "concurrently \"npm run sass\" \"npm run pug\" \"npm run imagemin\"",
    "build:dev": "concurrently \"npm run sass\" \"npm run sass:dev\" \"npm run pug:dev\" \"npm run imagemin\"",
    "pug": "pug -o demo/ src/demo/pug",
    "pug:dev": "pug -o demo/ -w -P src/demo/pug",
    "imagemin": "imagemin src/demo/images --out-dir demo/images"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pynklynn/keroseen-ui.git"
  },
  "keywords": [
    "scss",
    "sass",
    "ui",
    "css"
  ],
  "author": "Will Steinmetz <willsteinmetz@users.noreply.github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pynklynn/keroseen-ui/issues"
  },
  "homepage": "https://github.com/pynklynn/keroseen-ui#readme",
  "devDependencies": {
    "concurrently": "^5.2.0",
    "imagemin-cli": "^6.0.0",
    "node-sass": "^4.14.1",
    "node-static": "^0.7.11",
    "pug-cli": "^1.0.0-alpha6"
  },
  "dependencies": {}
}
